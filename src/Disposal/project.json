{
	"version": "0.1.0-*",
	"title": "Disposal",
	"description": "A utility class which makes correctly implementing IDisposable simpler than ever",
	"authors": [ "Nick Strupat" ],
	"packOptions": {
		"tags": [ "disposable", "dispose", "idisposable" ],
		"projectUrl": "https://github.com/NickStrupat/Disposal",
		"licenseUrl": "https://raw.githubusercontent.com/NickStrupat/Disposal/master/LICENSE"
	},
	"configurations": {
		"Debug": {
			"buildOptions": {
				"define": [ "DEBUG", "TRACE" ]
			}
		},
		"Release": {
			"buildOptions": {
				"define": [ "RELEASE", "TRACE" ],
				"optimize": true
			}
		}
	},
	"frameworks": {
		"net451": {
			"buildOptions": { "define": [ "DYNAMIC_ASSEMBLY_SAVEABLE" ] }
		},
		"netcoreapp1.0": {
			"dependencies": {
				"Microsoft.NETCore.App": {
					"type": "platform",
					"version": "1.0.0"
				},
				"System.Reflection.Emit.ILGeneration": "4.0.1",
				"System.Reflection.Emit.Lightweight": "4.0.1"
			},
			"imports": "dnxcore50"
		}
	}
}
